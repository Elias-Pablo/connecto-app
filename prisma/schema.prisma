datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            Int           @id @default(autoincrement()) 
  email         String        @unique
  password      String
  username      String        @unique 
  createdAt     DateTime      @default(now())
  updatedAt     DateTime      @updatedAt
}

